br = branch
co = checkout
df = diff
lg = log -p
l  = log
lp = log -p
stat = log --stat --summary
logg = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
pom  = push origin master
delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
